[{"data":1,"prerenderedAt":66},["Reactive",2],{"content-query-4cS1x0FBWU":3},{"_path":4,"_dir":5,"_draft":6,"_partial":6,"_locale":7,"title":8,"description":9,"image":10,"sequence":11,"category1":12,"category2":13,"link":14,"author":15,"head":16,"publishedAt":30,"toc":31,"body":32,"_type":61,"_id":62,"_source":63,"_file":64,"_extension":65},"/councilor/service/s3006","service",false,"","A7 二期內道路開闢工程變三道路會勘","文藝街紅線標示 - A7 二期內道路開闢工程變三道路會勘","/images/service/s3006-1.jpeg",3006,"A. 健全都市發展","A.4 強化區域交通規劃","https://www.facebook.com/sun0976315743","孫韻璇",{"meta":17},[18,20,22,24,27],{"name":19,"content":7},"keywords",{"name":21,"content":7},"robots",{"name":23,"content":15},"author",{"name":25,"content":26},"copyright","© 2024 A7新林口-社區網站",{"name":28,"content":29},"og:title","地方行政","2023-02-08T00:00:00.000Z",true,{"type":33,"children":34,"toc":57},"root",[35,43,49],{"type":36,"tag":37,"props":38,"children":40},"element","h2",{"id":39},"文藝街紅線標示-a7-二期內道路開闢工程變三道路會勘",[41],{"type":42,"value":9},"text",{"type":36,"tag":44,"props":45,"children":46},"p",{},[47],{"type":42,"value":48},"孫韻璇議員發文: 里長反應文青里二期內道路開闢工程變三道路拓寬後，文藝街街口也變寬了，因道路拓寬後許多車輛就停在上面，容易導致救災車輛無法順利通行。韻璇今日與文青里里長謝嘉仁、華亞別莊管委會、區公所、營造公司、技術顧問公司共同會勘，決議在兩側都將紅線畫上，才不會讓當初拓勘道路美意打折。",{"type":36,"tag":44,"props":50,"children":51},{},[52],{"type":36,"tag":53,"props":54,"children":56},"img",{"alt":55,"src":10},"s3006-1.jpeg",[],{"title":7,"searchDepth":58,"depth":58,"links":59},2,[60],{"id":39,"depth":58,"text":9},"markdown","content:councilor:service:s3006.md","content","councilor/service/s3006.md","md",1728293525520]