[{"data":1,"prerenderedAt":76},["Reactive",2],{"content-query-8RM3tWEhCW":3},{"_path":4,"_dir":5,"_draft":6,"_partial":6,"_locale":7,"title":8,"description":9,"image":10,"sequence":11,"category1":12,"category2":13,"link":14,"author":15,"head":16,"publishedAt":30,"toc":31,"body":32,"_type":71,"_id":72,"_source":73,"_file":74,"_extension":75},"/councilor/service/s3001","service",false,"","增設汽機車停車位","增設汽機車停車位 - 增加文青路及文學路汽機車停車位","/images/service/s3001-1.jpeg",3001,"A. 健全都市發展","A.4 強化區域交通規劃","https://www.facebook.com/sun0976315743","孫韻璇",{"meta":17},[18,20,22,24,27],{"name":19,"content":7},"keywords",{"name":21,"content":7},"robots",{"name":23,"content":15},"author",{"name":25,"content":26},"copyright","© 2024 A7新林口-社區網站",{"name":28,"content":29},"og:title","地方行政","2023-10-26T00:00:00.000Z",true,{"type":33,"children":34,"toc":67},"root",[35,43,49,54,59],{"type":36,"tag":37,"props":38,"children":40},"element","h2",{"id":39},"增設汽機車停車位-增加文青路及文學路汽機車停車位",[41],{"type":42,"value":9},"text",{"type":36,"tag":44,"props":45,"children":46},"p",{},[47],{"type":42,"value":48},"機捷 A7 體育大學站周邊區域近年迅速發展，人口移入也帶停車問題，加上基北北桃通勤月票使用率大幅提升，許多通勤族、轉乘族每天都要碰運氣使否能停到車位。😣",{"type":36,"tag":44,"props":50,"children":51},{},[52],{"type":42,"value":53},"韻璇在這次會期質詢交通局必須重視此問題，今天邀請#樂善里黃里長、交通局、警察局、區公所共同會勘，決議於樂善里文學路及樂善里文青路二側劃制機車、汽車停車約各 80 格，以便民眾停放改善停車問題‼️ 👍",{"type":36,"tag":44,"props":55,"children":56},{},[57],{"type":42,"value":58},"未來韻璇會再努力爭取更多相關措施 💪🏻 希望讓通勤族可以更方便！無須再為停車問題而煩惱囉",{"type":36,"tag":44,"props":60,"children":61},{},[62],{"type":36,"tag":63,"props":64,"children":66},"img",{"alt":65,"src":10},"s3001-1.jpeg",[],{"title":7,"searchDepth":68,"depth":68,"links":69},2,[70],{"id":39,"depth":68,"text":9},"markdown","content:councilor:service:s3001.md","content","councilor/service/s3001.md","md",1745926996618]